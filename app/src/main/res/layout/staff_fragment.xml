<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/button2"
    tools:context=".Fragment.StaffFragment">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarInStaffFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:navigationIcon="@drawable/abc_vector_test" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3InCellStaffFragment"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="@color/lightWhite"
        android:paddingTop="10sp"
        android:paddingBottom="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarInStaffFragment">

        <ImageView
            android:id="@+id/imageView3InCellStaffFragment"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_marginStart="10dp"
            android:background="#FFFFFF"
            android:contentDescription="@string/todo"
            android:src="@drawable/ps"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ImageContrastCheck" />

        <Button
            android:id="@+id/buttonEditInStaffFragment"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginEnd="5dp"
            android:background="@drawable/ic_baseline_create_24"
            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textViewStaffNameInStaffFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="@string/patientName"
            android:textAlignment="center"
            android:textColor="@color/brown"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imageView3InCellStaffFragment"
            app:layout_constraintStart_toStartOf="@+id/imageView3InCellStaffFragment"
            app:layout_constraintTop_toBottomOf="@+id/imageView3InCellStaffFragment" />

        <TextView
            android:id="@+id/textViewWeeklyCareHoursInStaffFragment"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="@dimen/textSizeRegular"
            app:layout_constraintBottom_toBottomOf="@+id/textViewInCellStaffFragment"
            app:layout_constraintStart_toEndOf="@+id/textViewInCellStaffFragment"
            tools:text="8h" />

        <TextView
            android:id="@+id/textView3InCellStaffFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/problemBehavior"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/textSizeRegular"
            android:visibility="invisible"
            app:layout_constraintStart_toStartOf="@+id/textViewInCellStaffFragment"
            app:layout_constraintTop_toBottomOf="@+id/textViewInCellStaffFragment"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/textViewStaffSymptomInStaffFragment"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="@dimen/textSizeRegular"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="@+id/textView3InCellStaffFragment"
            app:layout_constraintStart_toEndOf="@+id/textView3InCellStaffFragment"
            tools:text="暴言" />

        <TextView
            android:id="@+id/textViewStaffIdInStaffFragment"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textAlignment="viewStart"
            android:textAllCaps="false"
            android:textColor="@color/gray"
            android:textSize="@dimen/textSizeRegular"
            app:layout_constraintStart_toEndOf="@+id/textView16InCellStaffFragment"
            app:layout_constraintTop_toTopOf="@+id/textView16InCellStaffFragment"
            tools:ignore="RtlCompat,TextContrastCheck"
            tools:text="00000001" />

        <TextView
            android:id="@+id/textViewInCellStaffFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/WeeklyTherapyHours2"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/textSizeRegular"
            app:layout_constraintStart_toStartOf="@+id/textView4InCellStaffFragment"
            app:layout_constraintTop_toBottomOf="@+id/textView4InCellStaffFragment"
            tools:ignore="TextContrastCheck" />

        <TextView
            android:id="@+id/textView4InCellStaffFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="@string/sex"
            android:textAlignment="viewStart"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/textSizeRegular"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@+id/textView20InCellStaffFragment"
            app:layout_constraintTop_toBottomOf="@+id/textView20InCellStaffFragment"
            tools:ignore="RtlCompat,TextContrastCheck" />

        <TextView
            android:id="@+id/textViewStaffAgeInStaffFragment"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textAlignment="viewStart"
            android:textColor="@color/gray"
            android:textSize="@dimen/textSizeRegular"
            app:layout_constraintBottom_toBottomOf="@+id/textView20InCellStaffFragment"
            app:layout_constraintStart_toEndOf="@+id/textView20InCellStaffFragment"
            tools:ignore="RtlCompat,TextContrastCheck"
            tools:text="86" />

        <TextView
            android:id="@+id/textView16InCellStaffFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:gravity="center"
            android:text="@string/id"
            android:textAlignment="viewStart"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/textSizeRegular"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/imageView3InCellStaffFragment"
            app:layout_constraintTop_toTopOf="@+id/imageView3InCellStaffFragment"
            tools:ignore="RtlCompat,TextContrastCheck" />

        <TextView
            android:id="@+id/textView20InCellStaffFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="@string/age"
            android:textAlignment="viewStart"
            android:textColor="@color/colorPrimary"
            android:textSize="@dimen/textSizeRegular"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@+id/textView16InCellStaffFragment"
            app:layout_constraintTop_toBottomOf="@+id/textView16InCellStaffFragment"
            tools:ignore="RtlCompat,TextContrastCheck" />

        <TextView
            android:id="@+id/textViewStaffSexInStaffFragment"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="@dimen/textSizeRegular"
            app:layout_constraintBottom_toBottomOf="@+id/textView4InCellStaffFragment"
            app:layout_constraintStart_toEndOf="@+id/textView4InCellStaffFragment"
            tools:text="男" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/view8InCellStaffFragment"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="5dp"
        android:background="@color/colorSearch"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/view10InCellStaffFragment"
        app:layout_constraintTop_toTopOf="@+id/view10InCellStaffFragment" />

    <TextView
        android:id="@+id/view10InCellStaffFragment"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/ic____621"
        android:gravity="center|center_horizontal|center_vertical"
        android:text="@string/log"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3InCellStaffFragment"
        tools:ignore="TextContrastCheck" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/redyclerViewInStaffFragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="10dp"
        android:background="@color/cardview_shadow_end_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view8InCellStaffFragment" />

</androidx.constraintlayout.widget.ConstraintLayout>